		v.Global.scust.Set(v.Local.sret(0))

		f.Intrinsic.Control.CallSub(getcustomer)
		gui.frmARAging.lblCustomer.Caption(v.Local.sret)

		V.Local.Snew.Declare(string)
		f.Intrinsic.String.concat(v.Local.sretName," ~",v.Local.sret,v.Local.Snew)
		GUI.frmARAging.txtCustomerName.Text(v.Local.Snew)
	f.Intrinsic.Control.Else
		f.Intrinsic.UI.Msgbox("Beginning of file!")
	f.Intrinsic.Control.EndIf
Program.Sub.cmdPrevious_Click.End

Program.Sub.getCustomer.Start
	f.Intrinsic.Control.If(V.Global.sCust,<>,"")
		F.Intrinsic.Variable.UDTFlagAll(V.uGlobal.uInvoices)
		F.Intrinsic.Variable.UDTDeleteFlagged(V.uGlobal.uInvoices)
		F.Intrinsic.Variable.UDTFlagAll(V.uGlobal.uInvoices2)
		F.Intrinsic.Variable.UDTDeleteFlagged(V.uGlobal.uInvoices2)

		V.Local.SQUERY.Declare(STRING)
		v.Local.sval.Declare(string)
		v.Local.sval.Set(v.Global.sCust)

		'Get Address 3,4, and 5
		V.Local.SRET.Declare(STRING)
		f.Intrinsic.String.Concat("SELECT ADDRESS3,ADDRESS4,ADDRESS5,ADDRESS3_SHIP,ADDRESS4_SHIP,ADDRESS5_SHIP,ORDER_CURR,CATALOG_CURR ","",V.local.squery)
		f.intrinsic.string.concat(v.Local.squery,"FROM V_CUSTOMER_INTL WHERE CUSTOMER='",v.Local.sval,"'",v.Local.SQUERY)
		F.ODBC.Connection!con.ExecuteAndReturn(v.Local.SQUERY,v.Local.SRET)
		F.Intrinsic.String.Split(V.Local.SRET,"*!*",V.Local.SRET)

		GUI.frmARAging.txtBillAddress3.Text(V.Local.SRET(0))
		GUI.frmARAging.txtBillAddress4.Text(v.Local.SRET(1))
		gui.frmARAging.txtBillAddress5.Text(v.Local.SRET(2))
		gui.frmARAging.txtShipAddress3.text(v.Local.SRET(3))
		
		gui.frmARAging.txtShipAddress4.text(v.Local.SRET(4))
		gui.frmARAging.txtShipAddress5.text(v.Local.SRET(5))

		'Get Bill to Addresses 1,2
		f.Intrinsic.String.Concat("SELECT ADDRESS1,ADDRESS2,NAME_CUSTOMER,TELEPHONE ","",V.local.squery)
		f.intrinsic.string.concat(v.Local.squery,"FROM V_CUSTOMER_MASTER WHERE CUSTOMER='",v.Local.sval,"'",v.Local.SQUERY)
		F.ODBC.Connection!con.ExecuteAndReturn(V.Local.SQUERY,V.Local.SRET)
		F.Intrinsic.String.Split(V.Local.SRET,"*!*",V.Local.SRET)
		Gui.frmARAging.txtBillAddress1.Text(v.Local.SRET(0))
		gui.frmARAging.txtBillAddress2.Text(v.Local.sret(1))
		gui.frmARAging.txtBillName.text(v.Local.SRET(2))
		gui.frmARAging.txtBillPhone.text(v.local.sret(3))

		'Get Ship to addresses 1,2
		f.Intrinsic.String.Concat("SELECT ADDRESS1_SHIP,ADDRESS2_SHIP,NAME_CUSTOMER_SHIP,(T_COUNTRY +  AREA + TELE) AS TELEPHONE ","",V.local.squery)
		f.intrinsic.string.concat(v.Local.squery,"FROM V_CUSTOMER_SHIPTO WHERE CUSTOMER='",v.Local.sval,"'",v.Local.SQUERY)
		F.ODBC.Connection!con.ExecuteAndReturn(V.Local.SQUERY,V.Local.SRET)
		F.Intrinsic.String.Split(V.Local.SRET,"*!*",V.Local.SRET)
		Gui.frmARAging.txtShipAddress1.Text(v.Local.SRET(0))
		gui.frmARAging.txtShipAddress2.Text(v.Local.sret(1))
		Gui.frmARAging.txtShipName.Text(v.Local.SRET(2))
		Gui.frmARAging.txtShipPhone.text(v.local.sret(3))

		'Get aging date
		v.Local.dDate.Declare(date)
		v.Local.dDate.Set(V.Screen.frmARAging!dtpAgingDate.Value)

		'Calculate current aging total
		F.Intrinsic.String.Concat("SELECT SUM(IF( DATEDIFF(month,DATE_INVOICE,'",v.Local.ddate.PervasiveDate,"')=0,AMT_INVOICE,0)) ","",V.Local.SQUERY)
		F.Intrinsic.String.Concat(v.local.squery," FROM V_AR_OPEN_ITEMS WHERE CUSTOMER='",v.Global.sCust,"' ","",V.Local.SQUERY)
		v.Local.fCurrent.Declare(Float)
		f.ODBC.Connection!con.ExecuteAndReturn(v.Local.SQUERY,v.Local.fCurrent)
		gui.frmARAging.txtCurrentLocal.Text(v.local.fcurrent)

		'Calculate 1 month aging total
		F.Intrinsic.String.Concat("SELECT SUM(IF( DATEDIFF(month,DATE_INVOICE,'",v.Local.ddate.PervasiveDate,"')=1,AMT_INVOICE,0)) ","",V.Local.SQUERY)
		F.Intrinsic.String.Concat(v.local.squery," FROM V_AR_OPEN_ITEMS WHERE CUSTOMER='",v.Global.sCust,"' ","",V.Local.SQUERY)
		v.local.f1Mnt.declare(Float)
		f.ODBC.Connection!con.ExecuteAndReturn(v.Local.SQUERY,v.Local.f1Mnt)
		gui.frmARAging.txt1MonthLocal.Text(v.local.f1Mnt)

		'Calculate 2 month aging total
		F.Intrinsic.String.Concat("SELECT SUM(IF( DATEDIFF(month,DATE_INVOICE,'",v.Local.ddate.PervasiveDate,"')=2,AMT_INVOICE,0)) ","",V.Local.SQUERY)
		F.Intrinsic.String.Concat(v.local.squery," FROM V_AR_OPEN_ITEMS WHERE CUSTOMER='",v.Global.sCust,"' ","",V.Local.SQUERY)
		v.local.f2Mnt.declare(Float)
		f.ODBC.Connection!con.ExecuteAndReturn(v.Local.SQUERY,v.Local.f2Mnt)
		gui.frmARAging.txt2MonthLocal.Text(v.local.f2Mnt)

		'Calculate 3 month+ aging total
		F.Intrinsic.String.Concat("SELECT SUM(IF( DATEDIFF(month,DATE_INVOICE,'",v.Local.ddate.PervasiveDate,"')>=3,AMT_INVOICE,0)) ","",V.Local.SQUERY)
		F.Intrinsic.String.Concat(v.local.squery," FROM V_AR_OPEN_ITEMS WHERE CUSTOMER='",v.Global.sCust,"' ","",V.Local.SQUERY)
		v.local.f3Mnt.declare(Float)
		f.ODBC.Connection!con.ExecuteAndReturn(v.Local.SQUERY,v.Local.f3Mnt)
		gui.frmARAging.txt3MonthLocal.Text(v.local.f3Mnt)

		v.Local.ftotal.Declare(Float)
		f.Intrinsic.Math.Add(v.Local.f1Mnt,v.Local.f2Mnt,v.Local.ftotal)
		f.Intrinsic.Math.Add(v.Local.f3Mnt,v.Local.ftotal,v.Local.ftotal)
		F.Intrinsic.Math.Add(V.Local.fCurrent,V.Local.FTOTAL,V.Local.ftotal)

		gui.frmARAging.txtTotalAgingLocal.Text(v.local.ftotal)
		V.Global.lBoundary1.Set(0)
		V.Global.LBOUNDARY2.Set(0)
		V.Global.lboundary3.Set(0)

		'Calculate the month aging
		f.Intrinsic.Control.If(V.Screen.frmARAging!chkInclude.value,=,1)
			F.Intrinsic.String.Concat("SELECT INVOICE, DATE_INVOICE, DUE_DATE, AMT_INVOICE,REFERENCE,","",V.Local.SQUERY)
			F.Intrinsic.String.Concat(V.Local.SQUERY,"IF( DATEDIFF(day,DUE_DATE,'",v.Local.dDate.PervasiveDate,"')<",0,"  ,'X','')  AS 'CUR', ",V.Local.SQUERY)
			F.Intrinsic.String.Concat(V.Local.SQUERY,"IF( DATEDIFF(day,DUE_DATE,'",v.Local.dDate.PervasiveDate,"')>=0 AND DATEDIFF(day,DUE_DATE,'",v.Local.dDate.PervasiveDate,"')<",V.Global.lboundary1,",'X','')  AS '1MNT', ",V.Local.SQUERY)
			F.Intrinsic.String.Concat(V.Local.SQUERY,"IF( DATEDIFF(day,DUE_DATE,'",v.Local.dDate.PervasiveDate,"')>=",V.Global.lboundary1," AND DATEDIFF(day,DUE_DATE,'",v.Local.dDate.PervasiveDate,"')<",V.Global.lboundary2,",'X','')  AS '2MNT',",V.Local.SQUERY)
			F.Intrinsic.String.Concat(V.Local.SQUERY,"IF( DATEDIFF(day,DUE_DATE,'",v.Local.dDate.PervasiveDate,"')>=",V.Global.lboundary2,",'X','')  AS '3MNT', CUSTOMER from V_AR_OPEN_ITEMS ",V.Local.SQUERY)
			F.Intrinsic.String.Concat(V.Local.SQUERY,"where CUSTOMER = '",v.local.sval,"' ",V.Local.SQUERY)
			f.ODBC.Connection!con.OpenLocalRecordsetRO("rs2",v.Local.SQUERY)
			f.Intrinsic.Variable.LoadUDTFromRecordset("con","rs2","v.uGlobal.uInvoices2",true)
			f.ODBC.con!rs2.Close
		f.Intrinsic.Control.Else
			F.Intrinsic.String.Concat("SELECT Invoice, SUM(amt_invoice) as Total, Customer ","",V.Local.SQUERY)
			F.Intrinsic.String.Concat(V.Local.SQUERY," from v_ar_open_items WHERE CUSTOMER='",v.Local.sval,"'",v.Local.SQUERY)
			F.Intrinsic.String.Concat(v.Local.SQUERY," group by CUSTOMER, INVOICE order by customer",v.Local.SQUERY)
			F.ODBC.Connection!con.OpenLocalRecordsetRO("rsIV",v.Local.squery)
			f.Intrinsic.Variable.LoadUDTFromRecordset("con","rsIV",V.uGlobal.uInvoices,true)
			f.ODBC.con!rsIV.Close

			f.Intrinsic.Variable.UDTMultiFlag(V.uGlobal.uInvoices!Total,0)
			f.Intrinsic.Variable.UDTDeleteFlagged(V.uGlobal.uInvoices)

			v.Local.i2.Declare(long)
			v.Local.i2.Set(0)
			v.Local.iC.Declare(long)
			v.Local.iC.Set(0)
			v.Local.lRet.Declare(long)
			v.Local.lRet.Set(0)

			f.Intrinsic.Control.For(v.Local.i2,0,v.uGlobal.uInvoices.UBound,1)
				f.Intrinsic.String.Concat("select MONTH(DATE_INVOICE) FROM V_AR_OPEN_ITEMS WHERE CUSTOMER='",v.Local.sval,"' and INVOICE='",v.uGlobal.uInvoices(V.Local.i2)!InvoiceNo,"'",v.Local.squery)
				F.ODBC.Connection!con.ExecuteAndReturn(V.Local.SQUERY,V.Global.sMonth)
				f.Intrinsic.Control.CallSub(getmonthsdays)

				F.Intrinsic.String.Concat("SELECT INVOICE, DATE_INVOICE, DUE_DATE, AMT_INVOICE,REFERENCE,","",V.Local.SQUERY)
				F.Intrinsic.String.Concat(V.Local.SQUERY,"IF( DATEDIFF(day,DUE_DATE,'",v.Local.dDate.PervasiveDate,"')<",0,"  ,'X','')  AS 'CUR', ",V.Local.SQUERY)
				F.Intrinsic.String.Concat(V.Local.SQUERY,"IF( DATEDIFF(day,DUE_DATE,'",v.Local.dDate.PervasiveDate,"')>=0 AND DATEDIFF(day,DUE_DATE,'",v.Local.dDate.PervasiveDate,"')<",V.Global.lboundary1,",'X','')  AS '1MNT', ",V.Local.SQUERY)
				F.Intrinsic.String.Concat(V.Local.SQUERY,"IF( DATEDIFF(day,DUE_DATE,'",v.Local.dDate.PervasiveDate,"')>=",V.Global.lboundary1," AND DATEDIFF(day,DUE_DATE,'",v.Local.dDate.PervasiveDate,"')<",V.Global.lboundary2,",'X','')  AS '2MNT',",V.Local.SQUERY)
				F.Intrinsic.String.Concat(V.Local.SQUERY,"IF( DATEDIFF(day,DUE_DATE,'",v.Local.dDate.PervasiveDate,"')>=",V.Global.lboundary2,",'X','')  AS '3MNT', CUSTOMER from V_AR_OPEN_ITEMS ",V.Local.SQUERY)
				F.Intrinsic.String.Concat(V.Local.SQUERY,"where CUSTOMER = '",v.local.sval,"' ",V.Local.SQUERY)
				f.Intrinsic.String.Concat(v.Local.SQUERY,"AND INVOICE='",V.uGlobal.uInvoices(V.Local.i2)!InvoiceNo,"' ",V.Local.SQUERY)
				F.ODBC.Connection!con.OpenLocalRecordsetRO("rsNI",v.Local.SQUERY)
				f.Intrinsic.Variable.LoadUDTFromRecordset("con","rsNI",V.uGlobal.uInvoices2,true)
				F.ODBC.CON!rsNI.Close
			f.Intrinsic.Control.Next(v.Local.i2)
		f.Intrinsic.Control.EndIf
		gui.frmARAging.gsfgARDetails.LoadFromUDT(V.uGlobal.uInvoices2,"Invoice*!*DateInvoice*!*DateDue*!*Total*!*Reference*!*Cur*!*1MNT*!*2MNT*!*3MNT",2)
	f.Intrinsic.Control.EndIf

Program.Sub.getCustomer.End

Program.Sub.cmdNext_Click.Start
	'f.Intrinsic.Debug.InvokeDebugger
	'f.Intrinsic.Debug.Stop

	v.Local.lret.Declare(long)
	gui.frmARAging.lblLoading.Visible(True)
	f.Intrinsic.Variable.UDTSeek(v.uGlobal.uCustomers!CUSTID,v.Screen.frmARAging!lblCustomer.Caption,4,v.Local.lret)
	f.Intrinsic.Math.Add(v.Local.lret,1,v.Local.lret)
	v.Local.sret.Declare(string)
	f.Intrinsic.Control.If(v.Global.lTotalCustomers,<>,v.Local.lret)
		f.Intrinsic.Variable.udtpositiontostring(v.uGlobal.uCustomers,"CUSTID",V.Local.lret,v.Ambient.Tab,v.Local.sret)
		v.Local.sRetName.declare(string)
		f.Intrinsic.Variable.udtpositiontostring(v.uGlobal.uCustomers,"NAME",v.Local.lret,v.Ambient.Tab,v.Local.sretName)

		v.Global.scust.Set(v.Local.sret(0))
		gui.frmARAging.lblCustomer.Caption(v.Local.sret)

		V.Local.Snew.Declare(string)
		f.Intrinsic.String.concat(v.Local.sretName," ~",v.Local.sret,v.Local.Snew)
		GUI.frmARAging.txtCustomerName.Text(v.Local.Snew)

		f.Intrinsic.Control.CallSub(getcustomer)
	f.Intrinsic.Control.Else
		f.Intrinsic.UI.Msgbox("End of file!")
	f.Intrinsic.Control.EndIf
	gui.frmARAging.lblLoading.Visible(false)
Program.Sub.cmdNext_Click.End

