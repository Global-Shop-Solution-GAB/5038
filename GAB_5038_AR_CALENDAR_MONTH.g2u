Program.Sub.cmdPrintStatement_Click.Start
	V.Local.sCust.Declare(String)
	V.Local.sCust.Set(V.Screen.frmARAging!lblCustomer.Caption)

	V.Local.dDate.Declare(Date)
	V.Local.dDate.Set(V.Screen.frmARAging!dtpAgingDate.Value)

	V.Local.sDate.Declare(String)
	F.Intrinsic.String.Format(V.Local.dDate,"mmddyy",V.Local.sDate)

	V.Local.sDate2.Declare(String)
	F.Intrinsic.String.Format(V.Local.dDate,"YYYYMMDD",V.Local.sDate2)

	'This date is temporary until callwrapper fixed
	V.Local.dDate3.Declare(Date)
	V.Local.dDate3.Set(V.Ambient.Date)
	'F.Intrinsic.Date.DateAdd("D",365,V.Local.dDate3,V.Local.dDate3)

	V.Local.sDate3.Declare(String)
	F.Intrinsic.String.Format(V.Local.dDate3,"MMDDYY",V.Local.sDate3)
	'F.Intrinsic.String.Concat(V.Local.sDate3,"0",V.Local.sDate3)

	'Broken params
	'V.Local.sparams.Set("000001!*!ZZZZZZ!*!1071116!*!Y!*!Y!*!1!*!1130160!*! !*! !*! !*!")

	V.Local.sparams.Declare(String)
	F.Intrinsic.String.Concat(V.Local.sCust.TRIM,"!*!",V.Local.sCust.Trim,"!*!",V.Local.sDate,"!*!Y!*!Y!*!1!*!",V.Local.sDate,"!*! !*! !*!N!*!N!*!|~|CRYSTAL-OVERRIDE|~|",V.Local.sparams)

	F.Global.General.CallWrapperSync(3050,V.Local.sparams)

Program.Sub.cmdPrintStatement_Click.End

Program.Sub.cmdEmailStatement_Click.Start

	F.Intrinsic.Control.CallSub(Cmdprintstatement_click)

	V.local.sTerminal.declare(String)
	V.Local.sTerminal.Set(V.Caller.Terminal)

	V.Local.svalues.Declare(String)
	F.Intrinsic.String.Concat("TERMINAL*!*REPORTID*!*ATTN*!*TITLE1*!*TITLE2*!*TITLE3*!*TITLE4*!*Logo","",V.Local.svalues)
	F.Intrinsic.String.Split(V.Local.svalues,"*!*",V.Local.svalues)

	V.Local.sstring.Declare(String)
	F.Intrinsic.String.Concat(V.Caller.Terminal,"*!*000562*!* *!* *!* *!* *!* *!* ",V.Local.sstring)

	V.Local.sBIValues2.Declare(String)
	F.Intrinsic.String.Split(V.Local.sstring,"*!*",V.Local.sBIValues2)

	V.Local.sDate.Declare(String)
	F.Intrinsic.String.Format(V.Ambient.Date,"DDMMYY",V.Local.sDate)

	V.Local.sfile.Declare(String)
	F.Intrinsic.String.Concat(V.caller.TempDir,"\",V.Global.scust,"-",V.Local.sDate,".pdf",V.local.sfile)
	F.Global.BI.SaveReport("000562",1,V.Local.svalues,V.Local.sBIValues2,V.Local.sfile)

	V.Local.sMessag.Declare(String)
	F.Intrinsic.String.Concat("Dear Accounts Payable Department,",V.Ambient.NewLine,V.Ambient.NewLine,"Attached is your statement for your reference.",V.Ambient.NewLine,V.Ambient.NewLine,"Thanks and Best Regards",V.Local.sMessag)

	V.Local.sCustomerName.Declare(String)
	F.Intrinsic.String.Split(V.Screen.frmARAging!txtCustomerName.Text,"~",V.Local.sCustomerName)
	F.Intrinsic.String.UCase(V.Local.sCustomerName(0),V.Local.sCustomerName(0))

	V.Local.sHeader.Declare(String)
	F.Intrinsic.String.Concat(V.Local.sCustomerName(0)," - Customer Statement",V.Local.sHeader)

	V.Local.sEmail.Declare(String)
	V.Local.sEmail.Set(V.Screen.frmARAging!txtEmailAddress.Text)
	F.Intrinsic.String.Trim(V.Screen.frmARAging!txtEmailAddress.Text,V.local.sEmail)

	F.Intrinsic.Control.If(V.Local.sEmail,=,"")
		Function.Automation.MSOutlook.ComposeEmail(" ",v.Local.sHeader,v.Local.sMessag,v.Local.sfile)
	F.Intrinsic.Control.ELSE
		Function.Automation.MSOutlook.ComposeEmail(v.Local.semail,v.Local.sHeader,v.Local.sMessag,v.Local.sfile)
	F.Intrinsic.Control.ENDIF


Program.Sub.cmdEmailStatement_Click.End

