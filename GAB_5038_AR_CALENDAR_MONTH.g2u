Program.Sub.getMonthsDays.Start

	f.Intrinsic.Control.If(V.Global.sMonth,=,"1")
		v.Global.lboundary1.Set(31)
		v.Global.lboundary2.Set(28)
		v.Global.lboundary3.Set(31)
	f.Intrinsic.Control.ElseIf(V.Global.sMonth,=,"2")
		v.Global.lboundary1.Set(28)
		v.Global.lboundary2.Set(31)
		v.Global.lboundary3.Set(30)
	f.Intrinsic.Control.ElseIf(V.Global.sMonth,=,"3")
		v.Global.lboundary1.Set(31)
		v.Global.lboundary2.Set(30)
		v.Global.lboundary3.Set(31)
	f.Intrinsic.Control.ElseIf(V.Global.sMonth,=,"4")
		v.Global.lboundary1.Set(30)
		v.Global.lboundary2.Set(31)
		v.Global.lboundary3.Set(30)
	f.Intrinsic.Control.ElseIf(V.Global.sMonth,=,"5")
		v.Global.lboundary1.Set(31)
		v.Global.lboundary2.Set(30)
		v.Global.lboundary3.Set(31)
	f.Intrinsic.Control.ElseIf(V.Global.sMonth,=,"6")
		v.Global.lboundary1.Set(30)
		v.Global.lboundary2.Set(31)
		v.Global.lboundary3.Set(31)
	f.Intrinsic.Control.ElseIf(V.Global.sMonth,=,"7")
		v.Global.lboundary1.Set(31)
		v.Global.lboundary2.Set(31)
		v.Global.lboundary3.Set(30)
	f.Intrinsic.Control.ElseIf(V.Global.sMonth,=,"8")
		v.Global.lboundary1.Set(31)
		v.Global.lboundary2.Set(30)
		v.Global.lboundary3.Set(31)
	f.Intrinsic.Control.ElseIf(V.Global.sMonth,=,"9")
		v.Global.lboundary1.Set(30)
		v.Global.lboundary2.Set(31)
		v.Global.lboundary3.Set(30)
	f.Intrinsic.Control.ElseIf(V.Global.sMonth,=,"10")
		v.Global.lboundary1.Set(31)
		v.Global.lboundary2.Set(30)
		v.Global.lboundary3.Set(31)
	f.Intrinsic.Control.ElseIf(V.Global.sMonth,=,"11")
		v.Global.lboundary1.Set(30)
		v.Global.lboundary2.Set(31)
		v.Global.lboundary3.Set(31)
	f.Intrinsic.Control.ElseIf(V.Global.sMonth,=,"12")
		v.Global.lboundary1.Set(31)
		v.Global.lboundary2.Set(31)
		v.Global.lboundary3.Set(28)
	f.Intrinsic.Control.EndIf

	F.Intrinsic.Math.Add(V.Global.lBoundary1,V.Global.lboundary2,V.Global.lboundary2)
	F.Intrinsic.Math.Add(V.Global.lBoundary2,V.Global.lboundary3,V.Global.lboundary3)

Program.Sub.getMonthsDays.End

